   "source": [
    "n= int(input(\"Please enter the year: \"))\n",
    "if n%4==0 and n%100!=0:\n",
    "    print (f\"{n} is a leap year\")\n",
    "elif n%400==0:\n",
    "    print (f\"{n} is a leap year\")\n",
    "else:\n",
    "    print (f\"{n} is not a leap year\")"
   ]
